<div class="sign_in_container">
  <div class="form_container">
    <h2>Sign Up</h2>

    <form action="<%= users_url %>" method="post">
      <%= auth_token_input %>
      <label class="email">
        Email<br>
        <input class="email" type="text" name="user[email]" value="">
      </label><br>

      <label class="password">
        Password<br>
        <input class="password" type="password" name="user[password]" value="">
      </label><br>

      <input class="submit_form" type="submit" value="Sign Up">
    </form>
  </div>
</div>