<div class="sign_in_container">
  <div class="form_container">
    <h2 class="sign">Sign In</h2>

    <form action="<%= session_url %>" method="post">
      <%= auth_token_input %>
      <label class="email">
        Email<br>
        <input class="email" type="text" name="user[email]" value="guest">
      </label><br>

      <label class="password">
        Password<br>
        <input class="password" type="password" name="user[password]" value="password">
      </label><br>

      <input class="submit_form" type="submit" value="Sign In">
    </form>
  </div>
</div>